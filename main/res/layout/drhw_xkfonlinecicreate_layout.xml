<?xml version="1.0" encoding="utf-8"?>
<com.xsdk.ab_firstbase.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/drhw_sdk_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.xsdk.ab_firstbase.percent.PercentRelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <RelativeLayout
            android:id="@+id/rel_tile"
            android:layout_width="match_parent"
            android:layout_height="60dp">
            <ImageView
                android:id="@+id/tw_sdk_back_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/tw_sdk_back"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="20sp"
                android:textColor="@color/tw_black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/tw_kefu_online"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/tw_kftype_layout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_below="@+id/rel_tile"
            app:layout_heightPercent="8%"
            app:layout_widthPercent="100%"
            style="@style/tw_login_input_area">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:src="@drawable/drhw_complainttype"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/tw_dec_text_black"
                android:textColorHint="@color/tw_hint_text_black"
                android:id="@+id/tw_kf_tv"
                android:paddingTop="3dp"
                android:paddingBottom="3dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:hint="@string/tw_kefu_type"
                android:singleLine="true"
                android:layout_weight="1"
                android:imeOptions="flagNoExtractUi"
                style="@style/tw_option_edittext_style"/>
            <ImageView
                android:id="@+id/tw_kftype_more"
                android:paddingLeft="@dimen/tw_margin_size"
                android:paddingRight="@dimen/tw_margin_size"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/drhw_login_dropdown_icon"
                android:scaleType="center"
                android:contentDescription="@string/tw_img_desc"/>
        </LinearLayout>
        <View
            android:id="@+id/view_line1"
            android:background="@color/tw_main_text_black"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/tw_kftype_layout"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/tw_kftile_layout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_below="@+id/view_line1"
            app:layout_heightPercent="8%"
            app:layout_widthPercent="100%"
            style="@style/tw_login_input_area">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:src="@drawable/drhw_headtitle"/>
            <EditText
                android:textSize="12sp"
                android:textColor="@color/tw_dec_text_black"
                android:textColorHint="@color/tw_hint_text_black"
                android:id="@+id/tw_kftile_et"
                android:paddingTop="3dp"
                android:paddingBottom="3dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginLeft="10dp"
                android:hint="@string/tw_kefu_tile"
                android:singleLine="true"
                android:windowSoftInputMode="adjustResize|stateVisible"
                android:imeOptions="flagNoExtractUi"
                style="@style/tw_option_edittext_style"/>
        </LinearLayout>
        <View
            android:id="@+id/view_line2"
            android:background="@color/tw_main_text_black"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/tw_kftile_layout"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/tw_kfcontent_layout"
            android:background="@color/tw_color_f5f5f5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/view_line2"
            app:layout_heightPercent="40%"
            app:layout_widthPercent="100%">
            <EditText
                android:textSize="12sp"
                android:textColor="@color/tw_dec_text_black"
                android:textColorHint="@color/tw_hint_text_black"
                android:gravity="start"
                android:id="@+id/tw_kfcontent_et"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:lines="4"
                android:windowSoftInputMode="adjustResize|stateVisible"
                android:imeOptions="flagNoExtractUi"/>
        </LinearLayout>
        <RelativeLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/tw_kfbelow_layout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_below="@+id/tw_kfcontent_layout"
            app:layout_heightPercent="20%"
            app:layout_widthPercent="100%"
            style="@style/tw_login_input_area">
            <ImageView
                android:id="@+id/tw_phone_iv"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginLeft="5dp"
                android:src="@drawable/drhw_photoaccount"
                android:layout_centerVertical="true"/>
            <LinearLayout
                android:id="@+id/tw_kftip_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/tw_phone_iv"
                android:layout_centerVertical="true">
                <TextView
                    android:textSize="11sp"
                    android:textColor="@color/tw_dec_text_black"
                    android:id="@+id/tw_phone_tip1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tw_kefu_choose"/>
                <TextView
                    android:textSize="11sp"
                    android:textColor="@color/tw_dec_text_black"
                    android:id="@+id/tw_phone_tip2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1"
                    android:layout_toRightOf="@+id/tw_phone_tip1"
                    android:layout_centerVertical="true"/>
                <TextView
                    android:textSize="11sp"
                    android:textColor="@color/tw_dec_text_black"
                    android:id="@+id/tw_phone_tip3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tw_kefu_photo"
                    android:layout_toRightOf="@+id/tw_phone_tip2"
                    android:layout_centerVertical="true"/>
            </LinearLayout>
            <Button
                android:textSize="11dp"
                android:textColor="@color/tw_white"
                android:id="@+id/tw_submit"
                android:background="@drawable/drhw_zgywninebig"
                android:layout_width="60dp"
                android:layout_height="25dp"
                android:text="@string/tw_set_password_submit"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
    </com.xsdk.ab_firstbase.percent.PercentRelativeLayout>
</com.xsdk.ab_firstbase.percent.PercentRelativeLayout>
